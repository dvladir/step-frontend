{
  "name": "step-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build:core:lib": "ng build @exense/step-core",
    "build:core:schematics": "cd projects/step-core && npm run build",
    "build:core": "npm run build:core:lib && npm run build:core:schematics",
    "build:app": "ng build",
    "build": "npm run build:core && npm run build:app",
    "start:app": "ng serve",
    "start": "npm run build:core && npm run start:app",
    "test": "ng test",
    "postinstall": "cd src/app/angularjs && npm install",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc \"{src/app,projects/step-core/src}/!(angularjs)/**/*{.ts,.html,.scss}\"",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "14.0.1",
    "@angular/animations": "13.1.1",
    "@angular/common": "13.1.1",
    "@angular/compiler": "13.1.1",
    "@angular/core": "13.1.1",
    "@angular/forms": "13.1.1",
    "@angular/platform-browser": "13.1.1",
    "@angular/platform-browser-dynamic": "13.1.1",
    "@angular/router": "13.1.1",
    "@angular/upgrade": "13.1.1",
    "uuid": "8.3.2",
    "rxjs": "7.4.0",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.2",
    "@angular-devkit/schematics": "13.2.5",
    "@schematics/angular": "13.1.2",
    "@angular/cli": "13.1.2",
    "@angular/compiler-cli": "13.1.1",
    "@types/jasmine": "3.10.0",
    "@types/node": "12.11.1",
    "@types/angular": "1.7.4",
    "@types/uuid": "8.3.4",
    "husky": "7.0.4",
    "jasmine-core": "3.10.0",
    "karma": "6.3.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "^13.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.2",
    "typescript": "4.4.3",
    "copyfiles": "2.4.1"
  }
}
